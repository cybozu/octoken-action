  "license": "MIT",
  "engines": {
    "node": "16.x"
  },
  "main": "lib/main.js",
  "scripts": {
    "clean": "rimraf lib",
    "compile": "tsc",
    "compile:check": "tsc --noEmit",
    "build": "ncc build --source-map --license licnses.txt",
    "package": "run-s --print-label clean compile build",
    "eslint": "eslint 'src/**/*.ts'",
    "eslint:fix": "eslint --fix 'src/**/*.ts'",
    "prettier": "prettier --write '**/*.ts'",
    "prettier:check": "prettier --check '**/*.ts'",
    "lint": "run-p --print-label compile:check eslint prettier:check",
    "format": "run-s --print-label eslint:fix prettier",
    "test": "jest"
  },
  "dependencies": {
    "@actions/core": "1.10.1",
    "@actions/github": "4.0.0",
    "@octokit/auth-app": "2.11.0",
    "@octokit/core": "3.6.0"
  },
  "devDependencies": {
    "@cybozu/eslint-config": "22.0.2",
    "@types/jest": "29.5.11",
    "@types/node": "20.10.6",
    "@typescript-eslint/eslint-plugin": "6.16.0",
    "@vercel/ncc": "0.38.1",
    "eslint": "8.56.0",
    "eslint-config-prettier": "9.1.0",
    "jest": "29.7.0",
    "nock": "13.4.0",
    "npm-run-all": "4.1.5",
    "prettier": "3.1.1",
    "ts-jest": "29.1.1",
    "typescript": "5.3.3"
  }
}
