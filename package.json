  "license": "MIT",
  "engines": {
    "node": "16.x"
  },
  "main": "lib/main.js",
  "scripts": {
    "clean": "rimraf lib",
    "compile": "tsc",
    "compile:check": "tsc --noEmit",
    "build": "ncc build --source-map --license licnses.txt",
    "package": "run-s --print-label clean compile build",
    "eslint": "eslint 'src/**/*.ts'",
    "eslint:fix": "eslint --fix 'src/**/*.ts'",
    "prettier": "prettier --write '**/*.ts'",
    "prettier:check": "prettier --check '**/*.ts'",
    "lint": "run-p --print-label compile:check eslint prettier:check",
    "format": "run-s --print-label eslint:fix prettier",
    "test": "jest"
  },
  "dependencies": {
    "@actions/core": "1.10.0",
    "@actions/github": "4.0.0",
    "@octokit/auth-app": "2.10.5",
    "@octokit/core": "3.2.4"
  },
  "devDependencies": {
    "@cybozu/eslint-config": "18.0.0",
    "@types/jest": "29.2.5",
    "@types/node": "12.19.0",
    "@vercel/ncc": "0.26.2",
    "eslint": "8.31.0",
    "eslint-config-prettier": "8.6.0",
    "jest": "29.3.1",
    "nock": "13.0.5",
    "npm-run-all": "4.1.5",
    "prettier": "2.2.1",
    "ts-jest": "29.0.3",
    "typescript": "4.9.4"
  }
}
